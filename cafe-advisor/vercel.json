{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/logo.jpeg",
      "dest": "static/logo.jpeg"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "static/index.html"
    }
  ]
}
